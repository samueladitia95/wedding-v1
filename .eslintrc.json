{
	"extends": [
		"next/core-web-vitals",
		"prettier",
		"plugin:@typescript-eslint/recommended"
	],
	"root": true,
	"plugins": ["@typescript-eslint"],
	"rules": {
		"prefer-const": "error",
		"camelcase": "error",
		"dot-notation": "error",
		"@typescript-eslint/no-explicit-any": "off",
		"@typescript-eslint/array-type": "error"
	}
}
